    "jsx": {
      "markup.attributes": {
        "class*": "className"
      },
      "markup.valuePrefix": {
        "class*": "myStyles"
      }
    }
  },
  // ----
  // GIT SETTINGS
  // ----
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "always",
  "git.repositoryScanIgnoredFolders": ["node_modules"],
  // ----
  // NPM SETTINGS
  // ----
  "npm.enableScriptExplorer": true,
  "npm.scriptExplorerExclude": [],
