  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 6,
  "workbench.editor.splitSizing": "distribute",
  "workbench.editor.tabCloseButton": "left",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editor.wrapTabs": true,
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "Default Dark+",
  "workbench.preferredLightColorTheme": "Default Light+",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 20,
  "workbench.view.alwaysShowHeaderActions": true,
  "window.commandCenter": true,
  "window.title": "${dirty}${activeEditorShort}${separator}${folderName}${separator}${rootName}${separator}${profileName}${separator}",
  // ----
  // FILE SETTINGS
  // ----
  "files.autoSave": "afterDelay",
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "node_modules/": true,
    "USE_GITIGNORE": true
  },
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true,
    "**/env/**": true,
    "dist/": true,
    "env-*": true
  },
  // ----
  // ZEN MODE SETTINGS
  // ----
  "zenMode.centerLayout": false,
  // ----
  // EXPLORER SETTINGS
  // ----
  "explorer.autoRevealExclude": {
    "**/node_modules": true,
    "**/bower_components": true
  },
  "explorer.compactFolders": false,
  "explorer.excludeGitIgnore": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js, ${capture}.spec.ts, ${capture}.test.ts",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
    "*": "${basename}.*.${dirname}"
  },
  "explorer.openEditors.visible": 1,
  "explorer.sortOrder": "type",
  // ----
  // SEARCH SETTINGS
  // ----
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/env": true,
    "node_modules/": true,
    "*-lock.json": true,
    "*.lock": true
  },
  "search.showLineNumbers": true,
  "search.useGlobalIgnoreFiles": true,
  // ----
  // MARKDOWN SETTINGS
  // ----
  "markdown.occurrencesHighlight.enabled": true,
  "markdown.styles": [],
  "markdown.updateLinksOnFileMove.enabled": "always",
  "markdown.updateLinksOnFileMove.include": [
    "**/*.{md,mkd,mdwn,mdown,markdown,markdn,mdtxt,mdtext,workbook}",
    "**/*.{jpg,jpe,jpeg,png,bmp,gif,ico,webp,avif,tiff,svg,mp4}"
  ],
  "markdown.validate.duplicateLinkDefinitions.enabled": "error",
  "markdown.validate.enabled": true,
  "markdown.validate.ignoredLinks": [],
  "markdown.validate.referenceLinks.enabled": "error",
  "markdown.validate.unusedLinkDefinitions.enabled": "error",
  "markdown.editor.pasteUrlAsFormattedLink.enabled": true,
  // ----
  // JAVASCRIPT SETTINGS
  // ----
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.preferences.autoImportFileExcludePatterns": [],
  "javascript.preferences.quoteStyle": "single",
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "js/ts.implicitProjectConfig.checkJs": true,
  // ----
  // TYPESCRIPT SETTINGS
  // ----
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.tsdk": "./node_modules/typescript/lib",
  "typescript.tsserver.trace": "off",
  "typescript.updateImportsOnFileMove.enabled": "always",
  // ----
  // CSS SETTINGS
  // ----
  "css.lint.duplicateProperties": "warning",
  "css.lint.idSelector": "warning",
  "css.lint.ieHack": "warning",
  // ----
  // SCSS SETTINGS
  // ----
  "scss.lint.boxModel": "warning",
  "scss.lint.compatibleVendorPrefixes": "warning",
  "scss.lint.duplicateProperties": "warning",
  "scss.lint.idSelector": "warning",
  "scss.lint.important": "warning",
  // ----
  // NOTEBOOK SETTINGS
  // ----
  "notebook.cellToolbarLocation": {
    "default": "right"
  },
  // ----
  // INTERACTIVE WINDOW SETTINGS
  // ----
  "interactiveWindow.restore": false,
  // ----
  // TERMINAL SETTINGS
  // ----
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.enableBell": true,
  "terminal.integrated.lineHeight": 1.6,
  "terminal.integrated.localEchoExcludePrograms": ["vim", "vi", "nano", "tmux"],
  "terminal.integrated.minimumContrastRatio": 7,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.tabFocusMode": false,
  "terminal.integrated.tabs.defaultColor": "terminal.ansiMagenta",
  "terminal.integrated.tabs.title": "${cwdFolder}${separator}${workspaceFolder}${separator}${process}${separator}${sequence}${separator}${task}${separator}${cwdFolder}",
  "terminal.integrated.wordSeparators": " ()[]{}',\"`‚îÄ‚Äò‚Äô",
  "terminal.integrated.enableImages": true,
  // ----
  // AUDIO CUES SETTINGS
  // ----
  "audioCues.diffLineDeleted": "off",
  "audioCues.diffLineInserted": "off",
  "audioCues.diffLineModified": "off",
  "audioCues.lineHasBreakpoint": "off",
  "audioCues.lineHasError": "off",
  "audioCues.lineHasFoldedArea": "off",
  "audioCues.lineHasInlineSuggestion": "off",
  "audioCues.notebookCellCompleted": "off",
  "audioCues.notebookCellFailed": "off",
  "audioCues.onDebugBreak": "off",
  "audioCues.taskCompleted": "off",
  "audioCues.taskFailed": "off",
  "audioCues.terminalCommandFailed": "off",
  "audioCues.terminalQuickFix": "off",
  "audioCues.volume": 0,
  // ----
  // EMMET SETTINGS
  // ----
