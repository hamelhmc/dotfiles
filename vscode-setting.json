{
  // ----
  // EDITOR SETTINGS
  // ----
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "always",
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.addMissingImports": true,
    "source.organizeImports": true,
    "source.fixAll.eslint": true
  },
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line-thin",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "IBM Plex Mono, Cascadia Code, Jetbrains Mono, Fira Code, SF Mono, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.glyphMargin": false,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.lineHeight": 1.6,
  "editor.linkedEditing": true,
  "editor.links": false,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.occurrencesHighlight": false,
  "editor.overviewRulerBorder": false,
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "on"
  },
  "editor.renderLineHighlight": "gutter",
  "editor.renderWhitespace": "all",
  "editor.rulers": [100],
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 12,
  "editor.scrollBeyondLastLine": false,
  "editor.selectionHighlight": false,
  "editor.semanticTokenColorCustomizations": {},
  "editor.showFoldingControls": "always",
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggest.localityBonus": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Control flow",
        "scope": "keyword.control, keyword.control.flow",
        "settings": {
          "fontStyle": "italic bold"
        }
      },
      {
        "name": "Definitions",
        "scope": [
          "entity.name.type.class",
          "meta.definition.method",
          "meta.definition.property",
          "meta.definition.function",
          "entity.name.section.markdown"
        ],
        "settings": {
          "fontStyle": "italic bold"
        }
      },
      {
        "name": "Function/Method Calls and HTML attributes",
        "scope": [
          "meta.function-call",
          "keyword.operator.new.ts",
          "entity.other.attribute-name.html"
        ],
        "settings": {
          "fontStyle": "italic bold"
        }
      },
      {
        "name": "Other",
        "scope": [
          "keyword.control.import",
          "keyword.control.export",
          "keyword.control.from",
          "keyword.operator",
          "meta.type.annotation",
          "storage"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.unicodeHighlight.allowedCharacters": {},
  "editor.unicodeHighlight.allowedLocales": {
    "_os": true,
    "_vscode": true
  },
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 120,
  // ----
  // SOURCE CONTROL SETTINGS
  // ----
  "scm.defaultViewMode": "tree",
  "scm.diffDecorationsGutterPattern": {
    "added": false,
    "modified": true
  },
  // ----
